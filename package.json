{
    "name": "quinn-website",
    "version": "1.0.0",
    "description": "Quinn Tenorio's personal website",
    "main": "index.js",
    "scripts": {
        "clean": "rimraf _site",
        "build": "npm run sass && eleventy",
        "watch:eleventy": "eleventy --serve",
        "sass": "sass --no-source-map --style compressed src/assets/styles/main.scss _site/assets/styles/main.min.css",
        "watch:sass": "sass --watch src/assets/styles/main.scss:_site/assets/styles/main.min.css --no-source-map --style compressed",
        "start": "npm-run-all clean --parallel watch:*",
        "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build"
    },
    "author": {
        "name": "Quinn Tenorio",
        "email": "hey@quinntenor.io"
    },
    "license": "ISC",
    "devDependencies": {
        "rimraf": "^3.0.2"
    },
    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@notion-stuff/blocks-html-parser": "^5.1.0",
        "@notionhq/client": "^0.4.11",
        "canvas": "^2.8.0",
        "cloudinary": "^1.27.1",
        "dotenv": "^10.0.0",
        "highlight.js": "^11.3.1",
        "marked": "^4.0.8",
        "npm-run-all": "^4.1.5",
        "sass": "^1.45.1",
        "slugify": "^1.6.5"
    }
}
